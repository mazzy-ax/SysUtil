# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8

[*.xpo]
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.xpp]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
